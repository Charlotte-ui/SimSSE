{
  "name": "sim-sse",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "electron": "electron ./src/electron.dev",
    "package:win": "npm run build && cross-var electron-packager dist/simsse-editeur $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite ",
    "package:linux": "npm run build && cross-var electron-packager dist/simsse-editeur $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
    "package:osx": "npm run build && cross-var electron-packager dist/simsse-editeur $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "package:all": "npm run build && cross-var electron-packager dist/simsse-editeur $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/cdk": "^15.2.3",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.2.8",
    "@angular/material": "^15.2.3",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@ng-bootstrap/ng-bootstrap": "^14.1.0",
    "@popperjs/core": "^2.11.6",
    "angular-tag-cloud-module": "^14.0.0",
    "bootstrap": "^5.2.3",
    "echarts": "^5.4.2",
    "firebase": "^9.18.0",
    "gojs": "^2.3.7",
    "gojs-angular": "^2.0.6",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "ng-image-slider": "^7.0.0",
    "ngx-echarts": "^15.0.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.7",
    "@angular/cli": "~15.2.7",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/localize": "^15.2.8",
    "@types/jasmine": "~4.0.0",
    "concurrently": "^8.0.1",
    "cross-var": "^1.1.0",
    "electron": "^24.4.0",
    "electron-packager": "^17.1.1",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.5"
  }
}
